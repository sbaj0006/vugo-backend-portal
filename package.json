{
  "name": "vugo-streaming",
  "version": "1.0.0",
  "description": "Web Service of vugo-streaming backend",
  "main": "index.js",
  "scripts": {
    "start:admin": "cd services/admin-apis && node --max-old-space-size=4096 ../../node_modules/serverless/bin/serverless.js offline start --webpack-use-polling=500 --debugOffline",
    "start:user": "cd services/user-apis && node --max-old-space-size=4096 ../../node_modules/serverless/bin/serverless.js offline start --webpack-use-polling=500 --debugOffline --httpPort 4000 --websocketPort 4001 --lambdaPort 4002",
    "start:triggers": "cd services/triggers && node --max-old-space-size=4096 ../../node_modules/serverless/bin/serverless.js offline start --webpack-use-polling=500 --debugOffline",
    "start:user-trigger": "concurrently \"npm run start:user\"",
    "start:all": "concurrently \"npm run start:admin\" \"npm run start:user\"",
    "deploy:admin:dev": "cd services/admin-apis && sls deploy -v --stage dev",
    "deploy:user:dev": "cd services/user-apis && sls deploy -v --stage dev",
    "deploy:triggers:dev": "cd services/triggers && sls deploy -v --stage dev",
    "generate:migration:dev": "ts-node node_modules/typeorm/cli.js migration:generate -n base_structure -c dev",
    "generate:migration:local": "ts-node node_modules/typeorm/cli.js migration:generate -n base_structure -c local",
    "migrate:dev": "ts-node node_modules/typeorm/cli.js migration:run -c dev",
    "migrate:revert:dev": "ts-node node_modules/typeorm/cli.js migration:revert -c dev",
    "migrate:local": "ts-node node_modules/typeorm/cli.js migration:run -c local",
    "migrate:revert:local": "ts-node node_modules/typeorm/cli.js migration:revert -c local",
    "test": "node_modules/.bin/jest",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "serverless",
    "lambda",
    "aev",
    "back service"
  ],
  "author": "Peng",
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 105
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "@types/aws-lambda": "^8.10.61",
    "@types/bcrypt": "^3.0.0",
    "@types/crypto-js": "^4.0.1",
    "@types/hapi__joi": "^17.1.4",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.6.0",
    "@types/pg": "^7.14.4",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "amazon-cognito-identity-js": "^4.5.6",
    "await-to-js": "^2.1.1",
    "aws-sdk": "^2.817.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "cloudinary": "^1.22.0",
    "concurrently": "^5.3.0",
    "crypto": "^1.0.1",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "dynamoose": "^2.3.0",
    "eslint": "^7.7.0",
    "file-type": "^16.2.0",
    "fs": "0.0.1-security",
    "googleapis": "^67.0.0",
    "harsh": "^1.5.1",
    "increase-memory-limit": "^1.0.7",
    "jest": "^26.4.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "jwks-rsa": "^1.12.1",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "mime-types": "^2.1.27",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "prettier": "^2.0.5",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "serverless": "^1.79.0",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-offline-dotenv": "^0.3.0",
    "serverless-plugin-resource-tagging": "^1.0.11",
    "serverless-webpack": "^5.3.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.19",
    "stripe": "^8.131.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.25",
    "typeorm-aurora-data-api-driver": "^1.4.0",
    "typeorm-naming-strategies": "^1.1.0",
    "typescript": "^4.0.2",
    "uuid": "^8.3.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "serverless-deployment-bucket": "^1.3.0",
    "serverless-offline": "^3.33.0"
  }
}
